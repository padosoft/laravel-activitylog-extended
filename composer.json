{
    "name": "padosoft/laravel-activitylog-extended",
    "description": "An extension of spatie/laravel-activitylog for tracking model relations inside activites",
    "homepage": "https://github.com/padosoft/laravel-activitylog-extended",
    "keywords":
    [
        "padosoft",
        "log",
        "user",
        "activity",
        "laravel",
        "audit"
    ],
    "authors": [
        {
            "name": "Leonardo Padovani",
            "email": "helpdesk@padosoft.com",
            "homepage": "https://www.padosoft.com",
            "role": "Developer"
        }
    ],
    "require": {
        "spatie/laravel-activitylog": "^4.0"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "ext-json": "*",
        "orchestra/testbench": "^4.0|^5.0|^6.0|^7.0|^8.0",
        "phpunit/phpunit": "^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Padosoft\\Laravel\\ActivitylogExtended\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Padosoft\\Laravel\\ActivitylogExtended\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "license": "MIT",
    "extra": {
        "laravel": {
            "providers": [
                "Padosoft\\Laravel\\ActivitylogExtended\\ActivitylogExtendedServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
